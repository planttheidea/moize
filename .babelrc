{
    "env": {
        "lib": {
            "plugins": [
                ["@babel/plugin-proposal-private-methods", { "loose": false }]
            ],
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "loose": true
                    }
                ]
            ]
        },
        "test": {
            "plugins": ["@babel/plugin-proposal-class-properties"],
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "loose": true
                    }
                ],
                "@babel/preset-react"
            ]
        }
    },
    "presets": [
        "@babel/preset-typescript",
        [
            "@babel/preset-env",
            {
                "loose": true,
                "modules": false
            }
        ]
    ]
}
